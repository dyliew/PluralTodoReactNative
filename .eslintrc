{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": 2,
        "indent": [1, 4],
        "eol-last": 0,
        "comma-dangle": 0,
        "no-useless-constructor": 0,
        "no-trailing-spaces": 0,
        "react/jsx-indent-props": 0,
        "react/jsx-no-bind": 0
    }
}